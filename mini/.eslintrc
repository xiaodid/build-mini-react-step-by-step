{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "plugins": [
  ],
  "env": {
    "browser" : true
  },
  "globals": {
  },
  "rules": {
    "semi" : [0, "never"],
    "no-unexpected-multiline": "error",
    "max-len": [2, 1200, 2],
    "import/prefer-default-export": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0
  }
}
